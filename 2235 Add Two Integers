    def sum(self, num1, num2):
        """
        :type num1: int
        :type num2: int
        :rtype: int
        """

# Solution
class Solution(object):
    def sum(self, num1, num2):
        """
        :type num1: int
        :type num2: int
        :rtype: int
        """
        return num1 + num2